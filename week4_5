// 10250ë²ˆ

#include <stdio.h>
#include <stdlib.h>
int main()
{
    int T, H, W, N;
    scanf("%d", &T);
    for (int i = 0; i < T; i++)
    {
        int h = 0, num = 1;
        scanf("%d%d%d", &H, &W, &N);
        for (int j = 0; j < N; j++)
        {
            h++;
            if (h > H)
            {
                h = 1;
                num++;
            }
        }
        if (num < 10)
            printf("%d0%d\n", h, num);
        else
            printf("%d%d\n", h, num);
    }
}
