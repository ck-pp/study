#include <stdio.h>
#include <stdlib.h>

int list[] = {};
int main()
{
    int num, val;
    scanf("%d", &num);

    for (int i = 0; i < num; i++)
        scanf("%d", &list[i]);
    int max = -10000001, min = 10000001;
    
    for (int i = 0; i < num; i++)
    {
        if (list[i] > max)
            max = list[i];
        if (list[i] < min)
            min = list[i];
    }
    printf("%d %d\n", min, max);
}
